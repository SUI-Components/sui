{
  "compilerOptions": {
    "declaration": true,
    "jsx": "react-jsx",
    "module": "es6",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "baseUrl": ".",
    "outDir": "./lib",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "types": ["react", "node"]
  },
  "exclude": ["node_modules", "lib"],
  "typeAcquisition": {
    "enable": true
  }
}
