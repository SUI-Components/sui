{
  "name": "@s-ui/domain",
  "version": "2.0.0",
  "description": "SDK for creating domains",
  "main": "lib/index.js",
  "scripts": {
    "lib": "npx rimraf ./lib && ../../node_modules/.bin/babel ./src --out-dir ./lib",
    "prepare": "npm run lib",
