{
  "name": "@s-ui/i18n",
  "version": "1.34.0",
  "main": "./lib/index.cjs",
  "description": "> Isomorphic i18n service for browser and node.",
  "scripts": {
    "lib": "sui-js-compiler",
    "prepublishOnly": "rm -rf && npm run lib"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": { "@s-ui/js": "2" },
  "module": "./lib/index.js",
  "exports": {
    "./lib/console": {
      "require": "./lib/console.cjs",
      "import": "./lib/console.js"
    },
    "./lib/i18n": { "require": "./lib/i18n.cjs", "import": "./lib/i18n.js" },
    ".": { "require": "./lib/index.cjs", "import": "./lib/index.js" },
    "./lib/slugify": {
      "require": "./lib/slugify.cjs",
      "import": "./lib/slugify.js"
    },
    "./lib/adapters/default": {
      "require": "./lib/adapters/default.cjs",
      "import": "./lib/adapters/default.js"
    },
    "./lib/adapters/polyglot": {
      "require": "./lib/adapters/polyglot.cjs",
      "import": "./lib/adapters/polyglot.js"
    }
  }
}
